#include<iostream>
using namespace std;
int search(int array[],int n,int key)
{
    for(int i=0;i<=n;i++)
    {
        if(array[i]==key)
        {
            return i;
        }
    }return -1;
}
int main()
{
    int n,key;
    cin>>n>>key;
   int array[n];
    for(int i=0;i<=n;i++)
    {
        cin>>array[i];
    }cout<<search(array,n,key);
    return 0;
}
